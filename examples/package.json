{
  "name": "polynomialfi-examples",
  "version": "1.0.2",
  "description": "Example usage of the Polynomial SDK (polynomialfi)",
  "main": "basic-usage.ts",
  "scripts": {
    "start": "npx ts-node -P ./tsconfig.json basic-usage.ts",
    "dev": "nodemon --exec \"npx ts-node -P ./tsconfig.json\" basic-usage.ts",
    "debug": "TS_NODE_PROJECT=./tsconfig.json node --inspect-brk -r ts-node/register basic-usage.ts",
    "debug:watch": "TS_NODE_PROJECT=./tsconfig.json nodemon --exec \"node --inspect-brk -r ts-node/register\" basic-usage.ts",
    "refresh": "rm -rf node_modules package-lock.json && npm install polynomialfi@latest && npm install",
    "clean-install": "npm run refresh"
  },
  "keywords": [
    "polynomial",
    "perpetuals",
    "trading",
    "defi",
    "crypto",
    "sdk",
    "example"
  ],
  "author": "Polynomial Protocol",
  "license": "MIT",
  "dependencies": {
    "polynomialfi": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
